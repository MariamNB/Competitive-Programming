#include<bits/stdc++.h>
using namespace std;

#define ll long long
#define no cout << "NO\n"
#define yes cout << "YES\n"
#define endl cout << '\n'
#define vint vector<int>
#define vll vector<ll>
#define pb push_back
#define wl int t; cin >> t; while(t--)
#define sort(v) sort(v.begin(),v.end())
#define fast ios_base::sync_with_stdio(0),cin.tie(0),cout.tie(0)

int main() {

    wl{
        map <int, int> mp;
        wl{
            int x; cin >> x; mp[x]++;
        };
        int n = -1;
        for(auto &[x, y]: mp){
            if(y >= 3) {n = x; break;}
        }
        cout << n; endl;
    }

}
